    "description": "A time picker field for SilverStripe based on the jQuery UI Timepicker By François Gélinas http://fgelinas.com/code/timepicker/",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "forms",
        "time"
    ],
    "homepage": "http://github.com/pmachapman/silverstripe-timepickerfield",
    "authors": [
        {
            "name": "Peter Chapman",
            "email": "peter@conglomo.co.nz"
        }
    ],
    "license": "MIT",
    "require": {
        "silverstripe/framework": "^5.0",
        "composer/installers": "*"
    },
    "extra": {
        "expose": [
            "client/css/",
            "client/javascript/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
